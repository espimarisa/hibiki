<%# Compatibility & theming %>
<meta charset="utf-8">
<meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">
<meta content="#8E94F2" name="theme-color">
<meta content="#8E94F2" name="msapplication-navbutton-color">
<meta content="#8E94F2" name="apple-mobile-web-app-status-bar-style">
<%# Metadata %>
<meta content="website" property="og:type">
<meta content="summary" name="twitter:card">
<meta content="@HibikiApp" name="twitter:site">
<meta content="@HibikiApp" name="twitter:creator">
<meta content="hibiki, hibiki bot, hibikibot, hibiki discord, hibiki discord bot, discord bot" name="keywords">
<meta content="https://cdn.discordapp.com/avatars/<%= bot.user.id %>/<%= bot.user.avatar %>.png?size=512" property="og:image">
<meta content="https://cdn.discordapp.com/avatars/<%= bot.user.id %>/<%= bot.user.avatar %>.png?size=512" name="twitter:image">
<%# Resources %>
<link rel="icon" href="https://cdn.discordapp.com/avatars/<%= bot.user.id %>/<%= bot.user.avatar %>.png?size=512">
<link rel="stylesheet" href="/public/css/custom.css" crossorigin="anonymous">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma/css/bulma.min.css" crossorigin="anonymous">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma-prefers-dark/css/bulma-prefers-dark.min.css" crossorigin="anonymous">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free/css/all.min.css" crossorigin="anonymous">
