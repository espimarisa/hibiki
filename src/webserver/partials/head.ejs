<%# Compatibility & theming %>
<meta charset="utf-8" />
<meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible" />
<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport" />
<meta content="#8e94f2" name="theme-color" />
<meta content="#8e94f2" name="msapplication-navbutton-color" />
<meta content="#8e94f2" name="apple-mobile-web-app-status-bar-style" />
<%# Metadata %>
<meta content="website" property="og:type" />
<meta content="summary" name="twitter:card" />
<meta content="@sysdotini" name="twitter:site" />
<meta content="@sysdotini" name="twitter:creator" />
<meta content="hibiki, hibikiapp, hibiki app, hibiki bot, hibikibot, hibiki discord, hibiki discord bot, discord bot" name="keywords" />
<meta content="https://cdn.discordapp.com/avatars/<%= bot.user.id %>/<%= bot.user.avatar %>.png?size=128" property="og:image" />
<meta content="https://cdn.discordapp.com/avatars/<%= bot.user.id %>/<%= bot.user.avatar %>.png?size=128" name="twitter:image" />
<%# Resources %>
<link rel="preconnect" href="https://cdn.discordapp.com" crossorigin="anonymous" />
<link rel="preconnect" href="https://cdn.jsdelivr.net" crossorigin="anonymous" />
<link rel="icon" href="https://cdn.discordapp.com/avatars/<%= bot.user.id %>/<%= bot.user.avatar %>.png?size=128" type="image/png" />
<link rel="stylesheet" href="/public/css/theme.css" type="text/css" />
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@5.15.1/css/all.min.css" crossorigin="anonymous" />
