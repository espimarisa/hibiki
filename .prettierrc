{
  "plugins": ["prettier-plugin-sh", "prettier-plugin-pkg"],
  "arrowParens": "always",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "ignore",
  "proseWrap": "preserve",
  "quoteProps": "consistent",
  "trailingComma": "all",
  "printWidth": 140,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "useTabs": false
}
