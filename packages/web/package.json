{
  "private": true,
  "type": "module",
  "name": "@espimarisa/hibiki-web",
  "version": "2.0.0-alpha",
  "description": "Hibiki's landing page and dashboard.",
  "license": "zlib",
  "author": "Espi Marisa <contact@espi.me> (https://espi.me)",
  "homepage": "https://github.com/espimarisa/hibiki#readme",
  "packageManager": "pnpm@8.7.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/espimarisa/hibiki.git"
  },
  "bugs": {
    "url": "https://github.com/espimarisa/hibiki/issues"
  },
  "engines": {
    "node": ">=20.6.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "pnpm clean && vite build",
    "clean": "rimraf .svelte-kit",
    "check": "svelte-kit sync && svelte-check --ignore 'dist' --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-kit sync && vite dev",
    "lint": "prettier --c . && eslint .",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "test": "pnpm check && pnpm lint"
  },
  "dependencies": {
    "@auth/core": "^0.15.0",
    "@auth/sveltekit": "^0.3.7",
    "@discordjs/rest": "^2.0.1",
    "@fontsource/atkinson-hyperlegible": "^5.0.12",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/svelte-fontawesome": "^0.2.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "discord-api-types": "^0.37.56",
    "modern-normalize": "^2.0.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.25.0",
    "eslint": "^8.49.0",
    "eslint-plugin-svelte": "^2.33.1",
    "postcss": "^8.4.31",
    "sass": "^1.67.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-eslint-parser": "^0.33.0",
    "vite": "^4.4.9"
  }
}
